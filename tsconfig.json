{
  "compilerOptions": {
    "strict": true,
    "module": "CommonJS",
    "target": "ES6",
    "jsx": "preserve",
    "checkJs": true,
    "baseUrl": ".",
    "noEmit": true,
    "paths": {
      "baseurl": [
        "./"
      ],
      "components/*": [
        "./src/components/*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
